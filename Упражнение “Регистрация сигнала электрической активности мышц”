void setup() {
	Serial.begin(9600);
}
void loop() {
	int a2 = analogRead(A2); // Инициализируем переменную  типа int
	// в которую будут записываться данные с порта A2
	int a3 = analogRead(A3); // Инициализируем переменную типа int 
	// в которую будут записываться данные с порта A3
	Serial.println(a2); // Будем выводить данные в следующем формате:
	// Сенсор A2 | Сенсор A3
	Serial.print(a3);
	Serial.print(" | ");
	delay(4); // Установим задержку приема и вывода данных в 4 мс
}

